<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="roulette.overwatchroulette" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Splash"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NavBaseActivity"
            android:label="@string/title_activity_nave_base" >
        </activity>
        <activity
            android:name=".MapsActivity"
            android:label="@string/title_activity_maps" >

        </activity>
        <activity
            android:name=".FavoritesActivity"
            android:label="@string/title_activity_favorites" >
        </activity>
        <activity
            android:name=".StratRouletteActivity"
            android:label="@string/title_activity_strat_roulette" >
        </activity>
        <activity
            android:name=".SuggestionsActivity"
            android:label="@string/title_activity_suggestions" >
        </activity>
    </application>

</manifest>
